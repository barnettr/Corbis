<httpHandlers>
	<remove verb="*" path="*.asmx"/>
	<add verb="*" path="*.asmx" validate="false" type="System.Web.Script.Services.ScriptHandlerFactory, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
	<add verb="*" path="*_AppService.axd" validate="false" type="System.Web.Script.Services.ScriptHandlerFactory, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
	<add verb="GET,HEAD" path="ScriptResource.axd" validate="false" type="System.Web.Handlers.ScriptResourceHandler, System.Web.Extensions, Version=3.5.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35"/>
	<add verb="GET,HEAD" path="*.jpg" type="Corbis.Web.UI.Handlers.ImageHandler" validate="false" />
  <add verb="GET,HEAD" path="*.zip" type="Corbis.Web.UI.Handlers.ZipHandler" validate="false" />
  <!-- Dev Toolbar related handlers-->
	<!--<add verb="*" path="cookies.ashx" type="Corbis.Tools.WebToolbar.CookieHandler, Corbis.Tools.WebToolbar"/>
	<add verb="*" path="session.ashx" type="Corbis.Tools.WebToolbar.SessionHandler, Corbis.Tools.WebToolbar"/>
	<add verb="*" path="log.ashx" type="Corbis.Tools.WebToolbar.LogHandler, Corbis.Tools.WebToolbar"/>
	<add verb="*" path="permissions.ashx" type="Corbis.Tools.WebToolbar.PermissionsHandler, Corbis.Tools.WebToolbar"/>
	<add verb="*" path="toolbarconfig.ashx" type="Corbis.Tools.WebToolbar.ToolbarConfigHandler, Corbis.Tools.WebToolbar"/>
	<add verb="*" path="viewstate.ashx" type="Corbis.Tools.WebToolbar.ViewStateHandler, Corbis.Tools.WebToolbar"/>-->
</httpHandlers>